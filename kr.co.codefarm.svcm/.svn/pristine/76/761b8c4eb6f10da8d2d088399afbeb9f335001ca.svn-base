/*일정*/
.pro-info-box {
    position:relative;
}
.pro-info-box .pro-photo {
    float:left;
    display:table;
    width:70px;
    height:70px;
    border:1px solid #9ea4d0;
    border-radius:50%;
    margin-right:10px;
    -webkit-box-shadow: 2px 2px 11px 0px rgba(0,0,0,0.15);
    -moz-box-shadow: 2px 2px 11px 0px rgba(0,0,0,0.15);
    box-shadow: 2px 2px 11px 0px rgba(0,0,0,0.15);
}
.pro-info-box .pro-photo img {
    width:68px;
    height:68px;
    border-radius:50%;
}
.pro-info-box .pro-kind {
    display:block;
    font-size:16px;
    color:#7e84ae;
    font-weight:500;
    padding-top:12px;
}
.pro-info-box .pro-name {
    display:block;
    font-size:22px;
    color:#000;
    font-weight:500;
    padding-top:10px;
}