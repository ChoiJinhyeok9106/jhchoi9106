<script type="text/javascript">
 /*******************************************************************************************************************************************************************
 * vue start
 ********************************************************************************************************************************************************************/
var vm = new Vue({
	el: '#app',
	//사용할 데이터
	data: {
		defaultinfoList: new Dataset(),
		defaultinfoListPaging: new PagingSet(5, 5),
		updateDisabled: false,
		componentKey: 0,
		messageModal: new ModalSet('default-info-modal'),	// confirm 모달
        pSearchType: '1',
        pSearchArgv: '',
		mainListLoading: {id:'mainListLoading', val:false},
		titleImg: new SingleFileSet('SMN'),
		logoImg1: new SingleFileSet('SMN', true, 'LOGO1.svg'),
		logoImg2: new SingleFileSet('SMN', true, 'LOGO2.svg'),
		logoImg3: new SingleFileSet('SMN', true, 'LOGO.svg'),
		tabPage: 1,
	},
	//생성 될 때
	beforeCreate: function(){

	},
	//계산된 속성 표현 시
	computed: {

	},
	//데이터 감시
	watch: {
		//rowposition changed
		'defaultinfoList.rowposition': {
			handler: function (val, oldVal) {
				if(this.defaultinfoList.enableevent){
					var changeYn = this.defaultinfoList.dataCompare(oldVal);
					var rowType = this.defaultinfoList.getRowType(val);
					var oldRowType = this.defaultinfoList.getRowType(oldVal);

					if(!changeYn || oldRowType == this.ROWTYPE_INSERT){
						this.defaultinfoList.setDefaultRow(oldVal);
					}

					if(val != -1){
						var siteSeq = this.defaultinfoList.getColumn(val, 'SITE_SEQ');

						this.titleImg.init();
                    	this.logoImg1.init();
                    	this.logoImg2.init();
                    	this.logoImg3.init();

						if(rowType == this.ROWTYPE_NORMAL  || rowType == this.ROWTYPE_UPDATE){
							this.updateDisabled = true;
						}else{
							this.updateDisabled = false;
						}

						if(rowType != this.ROWTYPE_INSERT){
                        	this.titleImg.getFiles('SMN_DEFAULTINFO', siteSeq, 'titleImg', null);
                        	this.logoImg1.getFiles('SMN_DEFAULTINFO', siteSeq, 'logoImg1', null);
                        	this.logoImg2.getFiles('SMN_DEFAULTINFO', siteSeq, 'logoImg2', null);
                        	this.logoImg3.getFiles('SMN_DEFAULTINFO', siteSeq, 'logoImg3', null);
                        }
						this.$validator.reset();
					}
				}
			}
		},
		//row data changed
		'defaultinfoList.data': {
			handler: function (val, oldVal) {
				if(this.defaultinfoList.enableevent){
					var vm = this;
				  	val.filter( function( data, rowposition ) {
				      	return Object.keys(data	).some( function( colNm ) {
				          	var diff = data[colNm] !== vm.defaultinfoList.beforeData[rowposition][colNm];
				          	vm.defaultinfoList.getRowType(rowposition); // 데이터셋의 내용이 변경될 때마다 rowType을 변경.
				          	if(diff) {
				          		vm.defaultinfoList.beforeData[rowposition][colNm] = data[colNm];

				          		if(rowposition != -1 && colNm == 'SMS_USE_YN'){
				          			if(data[colNm] == 'N'){
				          				vm.defaultinfoList.currentRow.SMS_SEND_NO = '';
				          			}
				          		}
				          		vm.componentStateChange();
				          	}
				      	})
				  	});
				}
			},
			deep: true
	    },
		'defaultinfoListPaging.currentPage': {
			handler: function (val, oldVal) {
				this.setDefaultinfoList();
			}
	    },
	    'mainListLoading.val': {
	    	handler: function (val, oldVal) {
				this.LoadingOverlay($('#'+this.mainListLoading.id), val);
			}
	    },
	},
	//마운트 된 시점수행
	mounted: function(){
		this.setDefaultinfoList();
	},
	//함수
	methods: {
		/* 기본정보관리 리스트 조회 */
		setDefaultinfoList: function(){
			var self = this;

			if(isNull(this.pSearchArgv)) this.pSearchArgv = '';
            if(isNull(this.pSearchType)){
                this.pSearchType = '';
                this.pSearchArgv = '';
            }

            self.mainListLoading.val = true;
			var returnVal = defaultinfoApi.service(this.pSearchType, this.pSearchArgv, this.defaultinfoListPaging.currentPage, this.defaultinfoListPaging.getRowCnt(), false);

			returnVal.then(function(response) {
				self.defaultinfoList.setData(response.data);
				if(response.data.length > 0){
					var totalCnt = self.defaultinfoList.data[0].CNT;
					self.defaultinfoListPaging.setTotalCnt(totalCnt);
				}else{
					self.defaultinfoListPaging.setTotalCnt(0);
				}
			}).finally(function(){
				self.mainListLoading.val = false;
			});
		},
		 /* 검색 */
        search: function(){
        	var pageNo = this.defaultinfoListPaging.getCurrentPage();

        	if(pageNo == 1){
        		this.setDefaultinfoList();
        	}else{
        		this.defaultinfoListPaging.movePage(1);
        	}
        },
		/* 기본정보관리  리스트 신규행 추가 */
		newRow: function(){
			var addObj = {
                'SITE_SEQ': '',
                'SITE_NM': '',
                'BIZNO': '',
                'CORPNO': '',
                'CO_NM': '',
                'CO_ENM': '',
                'ADDR1': '',
                'ADDR2': '',
                'ZIPNO': '',
                'EOPTAE': '',
                'JONGMOK': '',
                'CAMPUS_URL_ID': '',
                'COPYRIGHT': '',
                'HOEWON_JOIN_URL': '',
                'GROUPWARE_SESSION_INSERT_URL': '',
                'GROUPWARE_EMAIL_LINK_URL': '',
                'GROUPWARE_EMAIL_LIST_URL': '',
                'GROUPWARE_EMAIL_CNT_URL': '',
                'PWD_FIND_URL': '',
                'SEND_MAIL': '',
                'SEND_NM': '',
                'DEFAULT_MAIL_TITLE': '',
                'SMS_USE_YN': '',
                'SMS_SEND_NO': '',
                'TEL_NO': '',
                'FAX_NO': '',
                'REMARK': '',
                'USE_YN': '',
                'DAEPYO_YN': '',
                'AGREE_ITEM1_NM': '',
                'AGREE_ITEM1_PILSU_YN': 'N',
                'AGREE_ITEM1_USE_YN': 'N',
                'AGREE_ITEM1_CONTENTS': '',
                'AGREE_ITEM2_NM': '',
                'AGREE_ITEM2_PILSU_YN': 'N',
                'AGREE_ITEM2_USE_YN': 'N',
                'AGREE_ITEM2_CONTENTS': '',
                'AGREE_ITEM3_NM': '',
                'AGREE_ITEM3_PILSU_YN': 'N',
                'AGREE_ITEM3_USE_YN': 'N',
                'AGREE_ITEM3_CONTENTS': '',
                'AGREE_ITEM4_NM': '',
                'AGREE_ITEM4_PILSU_YN': 'N',
                'AGREE_ITEM4_USE_YN': 'N',
                'AGREE_ITEM4_CONTENTS': '',
                'AGREE_ITEM5_NM': '',
                'AGREE_ITEM5_PILSU_YN': 'N',
                'AGREE_ITEM5_USE_YN': 'N',
                'AGREE_ITEM5_CONTENTS': '',
                'AGREE_ITEM6_NM': '',
                'AGREE_ITEM6_PILSU_YN': 'N',
                'AGREE_ITEM6_USE_YN': 'N',
                'AGREE_ITEM6_CONTENTS': '',
                'AGREE_ITEM7_NM': '',
