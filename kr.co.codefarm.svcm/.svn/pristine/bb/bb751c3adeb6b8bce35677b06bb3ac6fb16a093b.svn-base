                'AGREE_ITEM8_CONTENTS': '',
                'AGREE_ITEM9_NM': '',
                'AGREE_ITEM9_PILSU_YN': 'N',
                'AGREE_ITEM9_USE_YN': 'N',
                'AGREE_ITEM9_CONTENTS': '',
                'AGREE_ITEM10_NM': '',
                'AGREE_ITEM10_PILSU_YN': 'N',
                'AGREE_ITEM10_USE_YN': 'N',
                'AGREE_ITEM10_CONTENTS': '',
            };

			var newRowPosition = this.defaultinfoList.addRow(addObj);
		},
		/* 기본정보관리  리스트 저장,수정 */
		saveRow: function(){
			var validate = this.$validator.validateAll();
			var self = this;

			validate.then(function(response) {
				if(response){
					var rowposition = self.defaultinfoList.rowposition;
					var rowType = self.defaultinfoList.getRowType(rowposition);
					var returnVal = null;

					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM1_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM1_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM2_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM2_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM3_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM3_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM4_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM4_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM5_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM5_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM6_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM6_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM7_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM7_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM8_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM8_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM9_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM9_CONTENTS = '';
					if(isNull(self.defaultinfoList.currentRow.AGREE_ITEM10_CONTENTS)) self.defaultinfoList.currentRow.AGREE_ITEM10_CONTENTS = '';

					//신규
					if(rowType == ROWTYPE_INSERT){
						returnVal = defaultinfoApi.defaultinfoInsert(self.defaultinfoList.currentRow, true);
					}
					//수정
					else if(rowType == ROWTYPE_UPDATE){
						returnVal = defaultinfoApi.defaultinfoUpdate(self.defaultinfoList.currentRow, true);
					}else{
						notifySubmit('info', '기본정보 저장', '저장할 내역이 없습니다.', 'icon-message');
					}

					if(!isNull(returnVal)){
						returnVal.then(function(response) {
							var data = response.data;

							notifySubmit('success', '기본정보 저장', '저장이 완료되었습니다.', 'icon-save');
							systemApi.defaultinfoRefresh(null, false);
							self.setDefaultinfoList();
						}).catch(function(error) {
							notifySubmit('warning', '기본정보 저장', '저장에 실패하였습니다.', 'icon-caution');
						});
					}
				}else{
					notifySubmit('warning', '기본정보 저장', '입력항목을 확인해주세요.', 'icon-caution');
				}
			});
		},
		/* 기본정보관리  리스트 삭제 */
		deleteRow: function(row){
			var self = this;
			var rowType = self.defaultinfoList.getRowType(row);
			var returnVal = null;
			var siteSeq = self.defaultinfoList.getColumn(row, 'SITE_SEQ');

			self.defaultinfoList.deleteRow(row);
			if(rowType != ROWTYPE_INSERT){
				returnVal = defaultinfoApi.defaultinfoDelete(siteSeq, true);
				returnVal.then(function(response) {
					var data = response.data;

					self.titleImg.delFiles('SMN_DEFAULTINFO', siteSeq, 'titleImg', null, null, false);
					self.logoImg1.delFiles('SMN_DEFAULTINFO', siteSeq, 'logoImg1', null, null, false);
					self.logoImg2.delFiles('SMN_DEFAULTINFO', siteSeq, 'logoImg2', null, null, false);
					self.logoImg3.delFiles('SMN_DEFAULTINFO', siteSeq, 'logoImg3', null, null, false);

					notifySubmit('success', '기본정보 저장', '저장이 완료되었습니다.', 'icon-save');
					systemApi.defaultinfoRefresh(null, false);
					self.setDefaultinfoList();
					notifySubmit('success', '기본정보 삭제', '삭제가 완료되었습니다.', 'icon-trashcan');
				}).catch(function(error) {
					notifySubmit('warning', '기본정보 삭제', '삭제에 실패하였습니다.', 'icon-caution');
				});
			}else{
				notifySubmit('success', '기본정보 삭제', '삭제가 완료되었습니다.', 'icon-trashcan');
			}
		},
		/* 기본정보관리  리스트 상태 변경에 따른 컴포넌트 상태변경 */
		componentStateChange: function(){
			var rowposition = this.defaultinfoList.rowposition;
			var rowType = this.defaultinfoList.getRowType(rowposition);

			if(rowType == ROWTYPE_NORMAL || rowType == ROWTYPE_UPDATE || rowType == ROWTYPE_INSERT){
				$('#siteSeq').attr('disabled', true);
			}else{
				$('#siteSeq').attr('disabled', false);
			}
		},
		/*주소 검색*/
		postSearch: function(){
			new daum.Postcode({
		        oncomplete: function(data) {
		        	vm.defaultinfoList.setColumn(vm.defaultinfoList.rowposition,'ZIPNO',data.zonecode);
		        	vm.defaultinfoList.setColumn(vm.defaultinfoList.rowposition,'ADDR1',data.roadAddress);
		        	vm.componentKey++;
		        }
		    }).open();
		},
		rowClick: function(index){
			this.tabPage = 1;
			this.defaultinfoList.selectRow(index);
		},
		/* 저장할때 메세지창 띄우기*/
        messagePopup: function(pId, pData) {
            var title = '확인';
            var cont = '해당 작업을 수행하시겠습니까?';
            var pGb = 'confirm';

            switch(pId) {
	    		case 'ID_DELETE':
	    			title = '삭제';
	    			cont = "해당 항목을 삭제하시겠습니까?<br />(해당 건의 '대표자관리' 데이터도 삭제됩니다.)";
                    break;
	    	}

            this.messageModal.openModal(pGb, title, cont, 'small', pId, pData);
        },
	    messagePopupCallback: function(pGb, pId, pData) {	//confirm 확인 callback함수 (확인/취소 등 구분값, confirm 호출 구분 값, 전달 데이터)
	    	if(pGb!='confirm') return;

	    	switch(pId) {
	    		case 'ID_DELETE':
	    			this.deleteRow(pData);
	    			break;
	    	}
	    }
	}
});
