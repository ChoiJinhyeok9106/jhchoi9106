/*보드 메인*/
/*보드메인 sorting*/
.note-main-util {
	position:relative;
	z-index:20;
}
.note-list-sort {  
  position: absolute;
  top:0;
  right:40px;
  width: 150px;
  display: inline-block;
  min-height: 34px;  
  max-height: 34px;
  overflow: hidden;
  cursor: pointer;
  text-align: right;
  white-space: nowrap;
  color: #666666;
  outline: none;
  border: .06em solid transparent;
  transition: 0.3s all ease-in-out;
}
/* .note-list-sort input:focus + label { */
/*   background: #def; */
/* } */
.note-list-sort input {
  width: 1px !important;
  height: 1px !important;
  display: inline-block;
  position: absolute;
  opacity: 0.01;
  margin:0 !important;
}
.note-list-sort label {
  display: block;
  height: 34px;
  line-height: 34px;
  padding-left: 1em;
  padding-right: 2em;
  cursor: pointer;
  position: relative;
  transition: 0.3s color ease-in-out;
  margin-bottom:0;
}
.note-list-sort label:nth-child(2) {
  margin-top: 34px;
}
.note-list-sort input:checked + label {
  display: block;
  border-top: none;
  position: absolute;
  top: 0;
  width: 100%;
}
.note-list-sort input:checked + label:nth-child(2) {
  margin-top: 0;
  position: relative;
}
.note-list-sort::after {
  content: "";
  position: absolute;
  right: 0.8em;
  top: 15px;
  border: 0.3em solid #6972b1;
  border-color: #6972b1 transparent transparent transparent;
  transition: .4s all ease-in-out;
}
.note-list-sort.expanded {
  border: 1px solid #dcdff1;
  background: #fff;
  border-radius: 8px;
  padding: 0;
  box-shadow: rgba(0, 0, 0, 0.1) 3px 3px 5px 0px;
  max-height: 15em;
}
.note-list-sort.expanded label {  
}
.note-list-sort.expanded label:hover {
  color: #111;
  background-color:#f3f3f3;
}
.note-list-sort.expanded input:checked + label {
  color: #111;
}
.note-list-sort.expanded::after {
  transform: rotate(-180deg);
  top: 10px;
}

/*북마크*/
.bookmark {
    position: absolute;
    top:20px;
    left:20px;
    z-index:10;
}
.bookmark input[type="checkbox"] { /* 실제 체크박스는 화면에서 숨김 */
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip:rect(0,0,0,0);
    border: 0
}
.bookmark input[type="checkbox"] + label {
    display: inline-block;
    position: relative;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    font-size:14px;
    color:#666666;
    font-weight: 300;
    padding-left:30px;
    width:36px;
    height:36px;
    background-color:rgba(0,0,0,0.15);
    border-radius:50%;
    text-align:center;
}
.bookmark input[type="checkbox"] + label:before { /* 가짜 체크박스 */
    position: absolute;
    left:8px;
    top:8px;
    content: '\e923';
    display: inline-block;
    color:#fff;
    font-size:18px;
    font-family:'feather';
}
.bookmark input[type="checkbox"]:checked + label { /* 체크박스를 체크했을때 */
    background-color: #f35988;
}

