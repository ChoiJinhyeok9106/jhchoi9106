	$(document).ready(function() {
		/*왼쪽메뉴 펼치고 닫힘*/
		$('.menu-list-wrap .depth1').on('click',function(){
			if($(this).hasClass('on')) {
				$(this).removeClass('on');
				$(this).siblings('.menu-list').removeClass('on');
			} else {
				$(this).addClass('on');
				$(this).siblings('.menu-list').addClass('on');
			}
		});
		
		/*왼쪽메뉴 활성화*/
		$('.menu-list-wrap .menu a').on('click',function(){
			$('.menu-list-wrap .menu a').removeClass('active');
			$(this).addClass('active');
		});
		
		/*전체메뉴 닫기*/
		$('.portfolio-area-top .btn-total').on('click', function(){
			$('.total-menu-layer').addClass('on');
		})
		$('.total-menu-layer .btn-close').on('click', function(){
			$('.total-menu-layer').removeClass('on');
		})
		
		/*반응형에서 왼쪽메뉴 열고 닫기*/
		$('.left-zone .btn-mobile').on('click', function(){
			if($('.left-zone').hasClass('on')) {
				$('.left-zone').removeClass('on');
			} else {
				$('.left-zone').addClass('on');
			}
		})
		
		/*메뉴편집 클릭시*/
		$('.left-zone .btn-menu-edit').on('click', function(){
			$('.left-zone .menu-normal').removeClass('on');
			$('.pf-cont-zone .dark-bg').addClass('on');
			$('.left-zone .menu-edit').addClass('on');
			$('.right-zone .total-menu-layer').addClass('on');
		});
		$('.left-zone .btn-back').on('click', function(){
			$('.left-zone .menu-normal').addClass('on');
			$('.pf-cont-zone .dark-bg').removeClass('on');
			$('.left-zone .menu-edit').removeClass('on');
			$('.right-zone .total-menu-layer').removeClass('on');
		})
		
		/*순번변경 클릭시*/
		$('.left-zone .btn-menu-turn').on('click', function(){
			$('.left-zone .menu-normal').removeClass('on');
			$('.pf-cont-zone .dark-bg').addClass('on');
			$('.left-zone .menu-turn').addClass('on');
			$('.right-zone .total-menu-layer').addClass('on');
		});
		$('.left-zone .btn-back').on('click', function(){
			$('.left-zone .menu-normal').addClass('on');
			$('.pf-cont-zone .dark-bg').removeClass('on');
			$('.left-zone .menu-turn').removeClass('on');
			$('.right-zone .total-menu-layer').removeClass('on');
		})
		
		/*메뉴,순서 변경*/
		var menuM = $('.pf-cont-zone .left-zone .menu-turn-wrap > div .btn-box .btn-write');
		menuM.on('click', function(){
			$('.pf-cont-zone .left-zone .menu-turn-wrap > div [class^=icon]').removeClass('off');
			$('.pf-cont-zone .left-zone .menu-turn-wrap > div .txt').removeClass('off');
			$('.pf-cont-zone .left-zone .menu-turn-wrap > div .btn-box').removeClass('off');
			$('.pf-cont-zone .left-zone .menu-turn-wrap > div .modi-box').removeClass('on');
			$('.pf-cont-zone .left-zone .menu-turn-wrap > div').removeClass('on');
			$(this).parent().siblings('[class^=icon]').addClass('off');
			$(this).parent().siblings('.txt').addClass('off');
			$(this).parent().addClass('off');
			$(this).parent().siblings('.modi-box').addClass('on');
			$(this).parent().parent('.depth2').addClass('on');
		})
	});

