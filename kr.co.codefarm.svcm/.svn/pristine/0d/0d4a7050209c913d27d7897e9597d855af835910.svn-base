//모달레이어팝업
 function modal_open(el) {

     var temp = $('#' + el); //레이어의 id를 temp변수에 저장
     var bg = temp.prev().hasClass('bg'); //dimmed 레이어를 감지하기 위한 boolean 변수 

     if (bg) {
         $('.layer').fadeIn();
     } else {
         temp.fadeIn(); //bg 클래스가 없으면 일반레이어로 실행한다.
     }

     // 화면의 중앙에 레이어를 띄운다.

     //if (temp.outerHeight() < $(document).height()) temp.css('margin-top', '-' + temp.outerHeight() / 2 + 'px');
     //else temp.css('top', '0px');
     if (temp.outerWidth() < $(document).width()) temp.css('margin-left', '-' + temp.outerWidth() / 2 + 'px');
     else temp.css('left', '0px');

     temp.find('a.cbtn, a.layer_close, .cbtn2').click(function(e) {
         if (bg) {
             $('.layer').fadeOut();
         } else {
             temp.fadeOut(); //'닫기'버튼을 클릭하면 레이어가 사라진다.
         }
         e.preventDefault();
     });

     $('.layer .bg').click(function(e) {
         $('.layer').fadeOut();
         e.preventDefault();
     });

     // Prevent mousewheel scroll
     //$('html').on('wheel.modal mousewheel.modal', function () {
     //	return false; 
     //});
     // Disable browser scrolling
     $("html").css("overflow", "hidden");

     //Show & hide dialog
     $('.cbtn, .layer_close, .cbtn2').fadeIn().click(function() {
         $(this).fadeOut()
             // Enable scrollbars and mousewheel
             //	$('html').off('wheel.modal mousewheel.modal');
         $("html").css("overflow", "auto");
         $('.modal-layer-wrap').removeClass('on');
     });

     //modal background dark
     $('.modal-layer-wrap').find(temp).parent().addClass('on');

 }
	
$(document).ready(function() {
	//탭메뉴
    $(".tab-content").hide();
    $(".tab-content:first").show();
    $(".tab-type04 div a").click(function() {
        $(".tab-type04 div a").removeClass("active");
        $(this).addClass("active");
        $(".tab-content").hide()
        var activeTab = $(this).parent().attr("rel");
        $("#" + activeTab).fadeIn()
    });
    
	// 노트 메인 리스트 정렬
	$('.note-list-sort').click(function(e) {
	  e.preventDefault();
	  e.stopPropagation();
	  $(this).toggleClass('expanded');
	  $('#'+$(e.target).attr('for')).prop('checked',true);
	});
	$(document).click(function() {
	  $('.note-list-sort').removeClass('expanded');
	});
	
	//노트 메인 메뉴 레이어
	$('.note-list .btn-menu').click(function(e) {
		var $menu = $(this).siblings('.menu-layer');
		if ($menu.hasClass('on')) {
			$menu.removeClass('on');				
		} else {
			$(this).parent().siblings().children('.menu-layer').removeClass('on');
			$menu.addClass('on');
		}
	});
	$(document).mouseup(function(e){
		var container = $(".note-main-list .note-list .btn-menu");
		if(container.has(e.target).length === 0){
			$(".note-list .menu-layer").removeClass('on');
		}
	});
	
	//노트 라벨 색상 선택
	$('.color-sel label').on('click', function(){
		var color = $(this).attr('class');
		$('.label-set-box .label-list').attr('class','label-list');
		$('.label-set-box .label-list').addClass(color);
	});
	
	//초대관리
	$('.invite-list a').on('click', function(){
		$(this).siblings('a').removeClass('on');
	})
});	
</script>


