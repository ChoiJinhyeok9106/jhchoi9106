	data: {
		testdata : null,
	},
	//생성 될 때 
	beforeCreate: function(){
		
	},
	//계산된 속성 표현 시
	computed: {
		
	},
	//데이터 감시
	watch: {
		
	},
	//마운트 된 시점수행
	mounted: function(){
		var returnVal = userApi.user('1', '1', '박기태', '1', '5', false);
		var self = this;
		
		returnVal.then(function(response) {
			console.log(JSON.stringify(response.data));
			self.testdata = JSON.stringify(response.data);
