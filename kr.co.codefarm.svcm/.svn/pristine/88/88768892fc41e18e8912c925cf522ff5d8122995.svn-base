 ********************************************************************************************************************************************************************/

/*******************************************************************************************************************************************************************
 * javascript start 
 ********************************************************************************************************************************************************************/
 $(document).ready(function() {
	    
	    //포트폴리오 슬라이더
	    $('.cont-slide').on('init', function(event, slick) {
	        $(this).append('<div class="slick-counter"><span class="current"></span> / <span class="total"></span></div>');
	        $('.current').text(slick.currentSlide + 1);
	        $('.total').text(slick.slideCount);
	    })
	    .slick({
	        infinite: true,
	        arrows: true
	    })
	    .on('beforeChange', function(event, slick, currentSlide, nextSlide) {
	        $('.current').text(nextSlide + 1);
	    });
	     $('.action li a').click(function() {
	       var slideNo = $(this).parent().index();
	       $('.cont-slide').slick('slickGoTo', slideNo);
	       $('.action li a').removeClass('on');
	       $(this).addClass('on');
	     });
	     $('.slick-arrow').on('click', function(){
	        var current = ($('.slick-counter .current').text())-1;
	        $('.action li a').removeClass('on');
	        $('.action li').eq(current).find('a').addClass('on');
	     })     
	});

	$(document).ready(function() {
	    //창이 작아질때 왼쪽 메뉴 열고 닫기
	     $('#left-zone .btn-close').on('click', function(){
	        console.log($(this).parent());
	        if ($(this).parent().hasClass('on')) {
	             console.log(1);
	            $('.dark-layer').removeClass('on');
	            $(this).parent().removeClass('on');
	                
	        } else {
	            console.log(2);
	            $('.dark-layer').addClass('on');
	            $(this).parent().addClass('on');
	        }
	     })
	});
/*******************************************************************************************************************************************************************
 * javascript end 
 ********************************************************************************************************************************************************************/
</script>
