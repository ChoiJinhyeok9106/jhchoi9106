<script>
    $(document).ready(function() {
        var wwidth = $(window).width();    
        if (wwidth > 1280) {
            //holland 결과보기 적성탐색코드 펼치고, 감추기
            $('.holland-show-hide').on('click', function(){
                if($(this).parent().hasClass('active')){
                    $(this).text('펼쳐보기');
                    $(this).parent().removeClass('active');
                    $(this).siblings('.table-type03').fadeOut();
                    $(this).parent().stop().animate({'height':'60px'});
                    $('.holland-graph').appendTo('.right-zone');
                } else {
                    $(this).text('감추기');
                    $(this).siblings('.table-type03').fadeIn();
                    $(this).parent().stop().animate({'height':'100%'});
                    $(this).parent().addClass('active');
                    $('.holland-graph').insertAfter('.left-zone .box-type05:nth-child(3)');
                }
            });
        }
        if (wwidth <= 1279) {
            //holland 결과보기 적성탐색코드 펼치고, 감추기
            $('.holland-show-hide').on('click', function(){
                if($(this).parent().hasClass('active')){
                    $(this).text('펼쳐보기');
                    $(this).parent().removeClass('active');
                    $(this).siblings('.table-type03').fadeOut();
                    $(this).parent().stop().animate({'height':'60px'});
                    
                } else {
                    $(this).text('감추기');
                    $(this).siblings('.table-type03').fadeIn();
                    $(this).parent().stop().animate({'height':'100%'});
                    $(this).parent().addClass('active');
                    
                }
            });
        }
    });
</script>


