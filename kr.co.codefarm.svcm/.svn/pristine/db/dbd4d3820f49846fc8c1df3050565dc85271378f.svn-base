    padding-left:26px;
    line-height:1.4em;
    font-size:13px;
    color:#fff;
    font-family: 'noto sans kr';
}
.widget-type103 .top-line .local span:before {
    position: absolute;
    left:0;
    content:'\e98e';
    color:#fff;
    font-size:20px;
    font-family:'feather';
}
.widget-type103 .mid-line {
