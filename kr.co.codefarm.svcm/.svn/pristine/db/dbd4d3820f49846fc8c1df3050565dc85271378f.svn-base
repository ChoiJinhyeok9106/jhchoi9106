.widget_box .content_box .inner {
    padding:10px 30px 0 30px;
    height:100%;
}

/*위젯타입1 일반게시판*/
.widget-type01 {
    position:relative;
}
.widget-type01 ul {
    display:inline-block;
    width:100%;
    margin-bottom:10px;
}
.widget-type01 ul li {
    display:block;
    width:100%;
    padding:15px 0;
    border-bottom:1px dashed #d5d6e0;
}
.widget-type01 ul li:first-child {
    padding-top:0;
}
.widget-type01 ul li .date-box {
    float:left;
    display: inline-block;
    width:72px;
    height:72px;
    background-color:#676d9a;
    border-radius:50%;
    text-align:center;
    font-size:12px;
    color:#c6c9dd;
    font-family:'montserrat';
    font-weight:500;
    margin-right:25px;
}
.widget-type01 ul li .date-box.img-box {
  overflow: hidden;
}
.widget-type01 ul li .date-box.img-box img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.widget-type01 ul li .date-box .big {
    display: block;
    font-size:26px;
    color:#fff;
    padding:13px 0 2px;
}
.widget-type01 ul li .cont-box {
    overflow:hidden;
    text-align:left;
}
.widget-type01 ul li .cont-box .tit {
    font-size:16px;
    color:#333333;
    font-family:'noto sans kr';
    line-height: 1.4em;
    display: block; 
    white-space: nowrap; 
    overflow: hidden; 
    text-overflow: ellipsis;
}
.widget-type01 ul li .cont-box .txt {
    margin:5px 0 10px;
    font-size:14px;
    color:#555555;
    font-family:'noto sans kr';
    line-height: 1.4em;
    font-weight:300;
    display: block; 
    white-space: nowrap; 
    overflow: hidden; 
    text-overflow: ellipsis;
    white-space: normal; 
    height: 2.6em; 
    text-align: left; 
    word-wrap: break-word; 
    display: -webkit-box; 
    -webkit-line-clamp: 2; 
    -webkit-box-orient: vertical;
}
.widget-type01 ul li .cont-box .etc {
    position: relative;
    display: inline-block;
    color: #666;
    font-size:13px;
    padding-left: 22px;
    font-weight: 400;
    margin: 5px 20px 5px 0;
    font-family: 'montserrat';
}
.widget-type01 ul li .cont-box .etc:before {
    position: absolute;
    font-size: 18px;
    top: -3px;
    left: 0;
    color: #d0d0d0;
}
.widget-type01 ul li .cont-box .writer {
    font-size:13px;
    color:#364085;
    font-weight:300;
    font-style: italic;
}
.widget-type01 .btn.type02 {
    background-color:#2a358c;
} 

