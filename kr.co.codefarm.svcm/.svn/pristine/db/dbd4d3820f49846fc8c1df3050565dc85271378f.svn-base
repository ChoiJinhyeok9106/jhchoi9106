/*위젯타입8 상담현황*/
.widget-type08 {
    position:relative;
}
.widget-type08 .counsel-statue-box {
    background-color:#535c98;
    width:100%;
    height:80px;
    border-radius:8px;
    overflow:hidden;
    text-align: center;
    margin-bottom:25px;
}
.widget-type08 .counsel-statue-box .tit {
    display: inline-block;
    float: left;
    height:80px;
    text-align:center;
    font-size:18px;    
    color:#fff;
    line-height:1.4em;
    background-color:#373f79;
    padding:12px 25px;
}
.widget-type08 .counsel-statue-box .today-statue {
   display: inline-block;
   margin-top: 20px;
}
.widget-type08 .counsel-statue-box .today-statue > a {
    display:inline-block;
    position: relative;
    margin:0 30px;
    font-size:12px;
    color:#fff;
    text-align:center;
}
.widget-type08 .counsel-statue-box .today-statue > a .icon {
    display:block;
    font-size:26px;
    color:#fff;
    margin-bottom:3px;
}
.widget-type08 .counsel-statue-box .today-statue > a .new {
    display:inline-block;
    position:absolute;
    top:-5px;
    right:-5px;
    width:16px;
    height:16px;
    background-color:#f35a7e;
    text-align:center;
    font-size:10px;
    font-weight:700;
    line-height:16px;
    color:#fff;
    border-radius:50%;
}
.widget-type08 .graph-box {
    position: relative;
    width: 148px;
    height: 148px;
    margin: 30px auto;
}
.widget-type08 .graph-box .total {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: #4e68ca;
    font-size: 36px;
    font-weight: 600;
    font-family: 'montserrat';
    text-align: center;
}
.widget-type08 .graph-box .total .small {    
    font-size: 26px;    
}
.widget-type08 .graph-box .total .txt {
    display: block;
    color: #333;
    font-size: 14px;
    font-weight:300;
    letter-spacing: -2px;
    margin-top:5px;
}
.widget-type08 .graph-box .circle-bg {
  fill: none;
  stroke: #eee;
  stroke-width: 3.8;
}
.widget-type08 .graph-box .circle {
  fill: none;
  stroke-width: 3.8;
  stroke-linecap: round;
  animation: progress 1s ease-out forwards;
}
@keyframes progress {
  0% {
    stroke-dasharray: 0 100;
  }
}
.widget-type08 .graph-box .circular-chart {
  display: block;
  max-height: 250px;
}
.widget-type08 .graph-box .circular-chart.type02 {
  width:148px;
  margin:0 auto;
}
.widget-type08 .graph-box .circular-chart.type02 .circle-bg {
  stroke: #e6e6e6;
