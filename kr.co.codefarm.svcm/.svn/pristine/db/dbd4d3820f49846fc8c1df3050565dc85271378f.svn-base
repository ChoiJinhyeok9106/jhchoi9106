    position: relative;
    float:left;
    width:50%;
    height:100%;
}
.widget-type103 .mid-line .icon-wrap .icon-img {
    position: absolute;
    top:50%;
    left:50%;
    transform:translate(-50%, -50%);
}
.widget-type103 .mid-line .icon-wrap .icon-img img {
    max-width: none;
}
.widget-type103 .mid-line .icon-wrap .icon-img span {
