/*위젯타입6 마일리지*/
.widget-type06 {
    position:relative;
}
.widget-type06 .tit {
    text-align:center;
    font-size:24px;
    font-weight:700;
    color:#7e7ebc;
    font-family:'montserrat';
    margin-bottom:15px;
}
.widget-type06 .stit {
    text-align:center;
    font-size:14px;
    font-weight:300;
    color:#666;
    font-family:'noto sans kr';
    margin-bottom:25px;
}
.widget-type06 .mileage-box {
    position: relative;
    width:330px;
    height:100px;
    border-radius:5em;
    background-image: linear-gradient(45deg, #5155a3 , #9c387a, #c82764);
    margin:0 auto;
    -webkit-box-shadow: 10px 10px 20px 0px rgba(0,0,0,0.25);
    -moz-box-shadow: 10px 10px 20px 0px rgba(0,0,0,0.25);
    box-shadow: 10px 10px 20px 0px rgba(0,0,0,0.25);
    margin-bottom:20px;
}
.widget-type06 .mileage-box .initial {
    position: absolute;
    top:50%;
    left:25px;
    transform: translateY(-50%);
    display: inline-block;
    margin-right: 10px;
    border: 3px solid rgba(255,255,255,0.3);
    width: 34px;
    height: 34px;
    line-height: 30px;
    text-align: center;
    border-radius: 50%;
    font-size: 18px;
    color: rgba(255,255,255,0.3);
    font-weight: 700;
    font-family: 'montserrat';
}
.widget-type06 .mileage-box .mileage {
    position: absolute;
    top:50%;
    right:35px;
     transform: translateY(-50%);
    font-size:15px;
    color:#fff;
    font-weight:300;
    font-family: 'noto sans kr';
}
.widget-type06 .mileage-box .mileage .num {    
    font-size:50px;
    color:#fff;
    font-weight:300;    
    font-family: 'montserrat';
}
.widget-type06 .mileage-detail {
    display: block;
    width:100%;
    padding:16px 5px;
    border-bottom:1px solid #d8d9e2;
    overflow:hidden;
}
.widget-type06 .mileage-detail .blue {
    color:#3479ce;
}
.widget-type06 .mileage-detail .red {
    color:#ec4d4d;
}
.widget-type06 .mileage-detail .detail-left {
    float:left;
    text-align: left;
}
.widget-type06 .mileage-detail .detail-left .division-tit {
    font-size:13px;
    padding-right:10px;
    font-family:'noto sans kr';
}
.widget-type06 .mileage-detail .detail-left .division-txt {
    position: relative;
    font-size:13px;
    padding-left:10px;
    color:#777;
    font-family:'noto sans kr';
}
.widget-type06 .mileage-detail .detail-left .division-txt:before {
    position: absolute;
    content: '';
    top: 5px;
    left: 0;
    width: 1px;
    height: 11px;
    background-color: #e7e7e7;
}
.widget-type06 .mileage-detail .detail-left .text {
    position: relative;
    font-size:15px;
    color:#333;
    font-family:'noto sans kr';
    margin-top:8px;
}
.widget-type06 .mileage-detail .detail-right {
    float:right;
}
.widget-type06 .mileage-detail .detail-right .date {
