@media all and (max-width:1550px) {
    .main_inner {
        width: 100%;
        padding: 0 30px;
    }
    .notice_box .notice_box_inner .inner_cont>.main_carousel {
    	width:100%;
    	top:50%;
    	transform: translateY(-50%);
    	margin-top:0;
    }
}

@media all and (max-width:1024px) {
    .main_inner {
        width: 100%;
    }
    .notice_box .notice_box_inner .inner_cont {
        width: 33.333%;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(2) {
        width: 33.333%;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(3) {
        position: absolute;
        top: 10px;
        opacity: 0;
        right: 10px;
        width: 480px;
        height: 180px;
        background-color: #535da1;
        border-radius: 15px;
        transition: all .4s cubic-bezier(0.26, 0.71, 0.32, 0.79);
        z-index:89;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(3).on {
        opacity: 1;
        right: 10px;
        z-index: 120;
    }
    .notice_box .notice_box_inner .dark_layer.on {
        display: block;
    }
    .notice_box .notice_box_inner .inner_cont>.main_carousel {
        margin-top: 0;
        z-index: 110;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(3) .btn_close_m3 {
        display: block;
        z-index: 120;
    }
    .notice_box .notice_box_inner .m_btn_carousel {
        display: block;
    }
    .notice_box .notice_box_inner .inner_cont>.notice_list>li {
        padding-top: 65px;
    }
    .notice_box .notice_box_inner .inner_cont>.notice_list>li>.txt {
        font-size: 0;
    }
    .notice_box .notice_box_inner .inner_cont>.notice_list>li>.txt .eng {
        font-size: 12px;
    }
    .main_tab_box>li .tab_content .widget_list>.widget_box {
        width: 46.84%;
        margin: 1.58%;
    }
}

@media all and (max-width:768px) {
    .main_inner .top_logo .logo {
        left: 50%;
        transform: translateX(-50%);
    }
    .notice_box .notice_box_inner .m_btn_notice {
        display: block;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(1) {
        position: absolute;
        width: 330px;
        top: 10px;
        opacity: 0;
        left: 0;
        height: 180px;
        background-color: #3d5cad;
        border-top-right-radius: 15px;
        border-bottom-right-radius: 15px;
        transition: all .4s cubic-bezier(0.26, 0.71, 0.32, 0.79);
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(1).on {
        opacity: 1;
        z-index: 120;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(2) {
        width: 100%;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(3) {
        width: 330px;
        border-top-right-radius: 0;
        border-bottom-right-radius: 0;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(3).on {
        right: 0;
    }
    .notice_box .notice_box_inner .m_btn_carousel .text {
        display: none;
    }
    .notice_box .notice_box_inner .inner_cont>.main_carousel {
        width: 330px;
    }
    .slick-slide img {
        width: 330px;
    }
    .notice_box .notice_box_inner .inner_cont:nth-of-type(1) .btn_close_m1 {
        display: block;
        z-index: 120;
    }
    .main_tab_box>li .tab_content .widget_list .widget_box .title_box .icon {
        width: 48px;
        height: 48px;
        font-size:40px;
        line-height:48px;
    }
    .main_tab_box>li .tab_content .widget_list .widget_box .title_box .icon span {
        font-size: 40px;
        line-height: 48px;
    }
    .main_tab_box>li .tab_content .widget_list .widget_box .title_box {
        padding: 20px 0;
    }
    .main_tab_box>li .tab_content .widget_list .widget_box .title_box .widget_tit {
        padding-top: 5px;
        font-size: 18px;
    }
}

@media all and (max-width:610px) {
