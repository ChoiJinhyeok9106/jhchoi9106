#content_wrap {
    min-height: 100%;
    /*height: auto;*/
    z-index:600;
}

.main_wrap {
    position: relative;
    width: 100%;
    height: 100%;
    padding-right: 70px;
}

.main_wrap:before {
    position: absolute;
    content: '';
    top: 0;
    width: 100%;
    height: 64px;
    background-color: #fff;
}

.main_inner {
    position: relative;
    width: 1548px;
    margin: 0 auto;
}

.main_inner .top_logo {
    position: relative;
    width: 100%;
    height: 64px;
}

.main_inner .top_logo .logo {
    position: absolute;
    top: 12px;
    left: 20px;
}

.main_inner .top_logo .logo img {
    width: 120px;
}

.main_inner .notice_box {
    position: relative;
    display: inline-block;
    margin-top: 60px;
    width: 100%;
    border-radius: 10px;
    text-align: center;
    padding: 0 20px;
}

.notice_box .notice_box_slide {
    position: absolute;
    bottom: -6px;
    left: 50%;
    margin-left: -25px;
    width: 50px;
    height: 16px;
    background-color: #fff;
    border-radius: 20px;
    -webkit-box-shadow: -2px 2px 10px 0px rgba(0, 0, 0, 0.1);
    -moz-box-shadow: -2px 2px 10px 0px rgba(0, 0, 0, 0.1);
    box-shadow: -2px 2px 10px 0px rgba(0, 0, 0, 0.1);
    z-index: 120;
}

.notice_box .notice_box_slide:before {
    position: absolute;
    content: '\e935';
    left: 50%;
    margin-left: -8px;
    font-family: 'Feather';
    font-size: 16px;
    color: #444d8e;
}

.notice_box .notice_box_slide.on:before {
    position: absolute;
    content: '\e92e';
    left: 50%;
    margin-left: -8px;
    font-family: 'Feather';
    font-size: 16px;
    color: #444d8e;
}

.notice_box.stu .notice_box_inner {
    background: linear-gradient( to right, #4873ca, #1e1a56);
}

.notice_box.pro .notice_box_inner {
