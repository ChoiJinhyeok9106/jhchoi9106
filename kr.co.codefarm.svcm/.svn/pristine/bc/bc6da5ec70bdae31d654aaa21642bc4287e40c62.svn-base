}

.notice_box .notice_box_inner .inner_cont>.main_carousel>.slick-next {
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -15px;
    z-index: 100;
    width: 30px;
    height: 30px;
    color: transparent;
    font-size: 0px;
}

.notice_box .notice_box_inner .inner_cont>.main_carousel>.slick-next:before {
