<script type="text/javascript">
/********************************************************************************************************************************************************************
 * vue start 
 ********************************************************************************************************************************************************************/
var vm = new Vue({
	el: '#app',
	//사용할 데이터
	data: {

	},
	//생성 될 때 
	beforeCreate: function(){

	},
	//계산된 속성 표현 시
	computed: {
		
	},
	//데이터 감시
	watch: {

	},
	//마운트 된 시점수행
	mounted: function(){
		this.drawChart1();
		this.drawChart2();
		this.drawChart3();
	},
	//함수
	methods: {
		drawChart1 : function () {
			var name = ['인성봉사', '글로컬', '창의융복합', '자기계발'];
			var dept = ['공과대학', '소프트웨어융합대학', '경영대학', '건축·디자인대학', '인문사회대학', '미래융합대학'];
			
			var dataList = [];
			for(var i = 0 ; i < dept.length ; i++) {
				var list = [];
				for(var j = 0 ; j < name.length ; j++) {
					var weight = Number(Math.random() * 100).toFixed(2);
					list.push(weight);
				}
				
				var obj = {
					'name' : dept[i],
					'data' : list
				};
				dataList.push(obj);
			}
			
			$("chart1").empty();
			var container = document.getElementById('chart1');
			var data = {
			    categories: name,
			    series: dataList
			};
			var options = {
		        chart: {
		            width: 1160,
		            height: 600
		        },
		        series: {
		            showDot: false,
		            showArea: true,
		            pointWidth: 1
		        },
		        plot: {
		            type: 'spiderweb'
		        },
		        legend: {
		        	 align: 'right'
		        },
		        chartExportMenu: {
		            visible: false
		        },
			    tooltip: {
			        suffix: '점'
			    }
			};
			var theme = {
			    series: {
			    	dot: {
			            fillColor: null,
			            radius: 2,
			            strokeWidth: 0,
			            hover: {
			              fillColor: null,
			              radius: 4,
			              strokeOpacity: 1,
			              strokeWidth: 2
			            }
			        },
			        colors: ['#855ef7', '#22b8d0', '#fbb005', '#fa5252', '#0d8596']
			    },
			    legend: {
			    	label: {
			            fontSize: 14,
			            fontFamily: 'Noto sans KR',
			            fontWeight: 'regular',
			            color: '#000'
			        }
			    },
			    plot: {
			    	label: {
			    		fontSize: 16,
				    	fontFamily: 'Noto sans KR'
			    	}
			    }
			};
			
			
			tui.chart.registerTheme('radialTheme', theme);
			options.theme = 'radialTheme';
			tui.chart.radialChart(container, data, options);
		},
		drawChart2 : function () {
			var name = ['인성봉사', '글로컬', '창의융복합', '자기계발'];
			var dept = ['학습', '상담', '취업', '창업', '진로'];
			
			var dataList = [];
			for(var i = 0 ; i < dept.length ; i++) {
				var list = [];
				for(var j = 0 ; j < name.length ; j++) {
					var weight = Number(Math.random() * 100).toFixed(2);
					list.push(weight);
				}
				
				var obj = {
					'name' : dept[i],
					'data' : list
				};
				dataList.push(obj);
			}
			
			$("chart2").empty();
			var container = document.getElementById('chart2');
			var data = {
			    categories: name,
			    series: dataList
			};
			var options = {
		        chart: {
		            width: 1160,
		            height: 600
		        },
		        series: {
		            showDot: false,
		            showArea: true,
		            pointWidth: 1
		        },
		        plot: {
		            type: 'spiderweb'
		        },
		        legend: {
		            align: 'right'
		        },
		        chartExportMenu: {
		            visible: false
		        },
			    tooltip: {
			        suffix: '점'
			    }
			};
			var theme = {
			    series: {
			    	dot: {
			            fillColor: null,
			            radius: 2,
			            strokeWidth: 0,
			            hover: {
			              fillColor: null,
			              radius: 4,
			              strokeOpacity: 1,
			              strokeWidth: 2
			            }
			        },
			        colors: ['#855ef7', '#22b8d0', '#fbb005', '#fa5252', '#0d8596']
			    },
			    legend: {
		            align: 'right',
			    	label: {
			            fontSize: 14,
			            fontFamily: 'Noto sans KR',
			            fontWeight: 'regular',
			            color: '#000'
			        }
			    },
			    plot: {
			    	label: {
			    		fontSize: 16,
				    	fontFamily: 'Noto sans KR'
			    	}
			    }
			};
			
			
			tui.chart.registerTheme('radialTheme', theme);
			options.theme = 'radialTheme';
			tui.chart.radialChart(container, data, options);
		},
		drawChart3 : function () {
			var name = ['학습', '상담', '취업', '창업', '진로'];
			var dept = ['참여학생', '프로그램'];
			
			var dataList = [];
			for(var i = 0 ; i < dept.length ; i++) {
				var list = [];
				for(var j = 0 ; j < name.length ; j++) {
					var weight = Number(Math.random() * 100).toFixed(2);
					list.push(weight);
				}
				
				var obj = {
					'name' : dept[i],
					'data' : list
				};
				dataList.push(obj);
			}
			
			$("chart3").empty();
			var container = document.getElementById('chart3');
			var data = {
			    categories: name,
			    series: dataList
			};
			
			var options = {
			    chart: {
		            width: 1160,
		            height: 400,
			        format: '1,000'
			    },
			    yAxis: {
			    	labelInterval: 5
			    },
			    xAxis: {
			        min: 0,
//  			        max: 30,
			    },
			    series: {
			        showLabel: true
			    },
			    chartExportMenu: {
			        visible: false
			    }
			};
			var theme = {
			    series: {
			    	dot: {
			            fillColor: null,
			            radius: 2,
			            strokeWidth: 0,
			            hover: {
			              fillColor: null,
			              radius: 4,
			              strokeOpacity: 1,
			              strokeWidth: 2
			            }
			        },
			        line: {
			        	pointWidth: 1
			        },
			    	colors: ['#855ef7', '#22b8d0', '#6079d4', '#f8be76', '#c09fee', '#6381f0']
			    },
			    xAxis: {
			    	label: {
			    		fontSize: 15,
				    	fontFamily: 'Noto sans KR'
			    	}
			    },
			    yAxis: {
			    	label: {
			    		fontSize: 16,
				    	fontFamily: 'Noto sans KR'
			    	}
			    },
			    legend: {
			    	label: {
			            fontSize: 15,
			            fontFamily: 'Noto sans KR'
			        }
			    }
			};
			
			tui.chart.registerTheme('barTheme', theme);
			options.theme = 'barTheme';
			
			tui.chart.barChart(container, data, options);
		},
	}
});
/*******************************************************************************************************************************************************************
 * vue end 
 ********************************************************************************************************************************************************************/

 
 /*******************************************************************************************************************************************************************
  * tui script start 
  ********************************************************************************************************************************************************************/

 /*******************************************************************************************************************************************************************
  * tui script end 
  ********************************************************************************************************************************************************************/
  
 
