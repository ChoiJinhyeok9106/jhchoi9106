@charset "utf-8";
.sub_cont_wrap .store_top {
    height: 360px;
    min-height: auto;
    padding: 0;
    background: url(/app/design/images/store/appstore_tit_bg.jpg) center center no-repeat;
    text-align: center;
    margin-bottom: 20px;
}

.store_top h3 {
    font-size: 42px;
    font-weight: 300;
    color: #fff;
    margin: 80px 0 26px 0;
}

.store_top h3 strong {
    font-weight: 600;
}

.store_top>p {
    font-size: 17px;
    font-weight: 200;
    line-height: 24px;
    color: #fff;
    text-align: center;
}

.store_top .search-wrap {
    margin-top: 74px;
}

.store_top .search-wrap .icon-search:before {
    font-size: 30px;
    color: #fff;
}

.store_top .search-wrap select {
    min-width: 150px;
}

.store_top .search-wrap input[type="text"] {
    min-width: 300px;
}

.store_top .search-wrap .btn {
    background: #fff;
    font-size: 15px;
    color: #666faf;
}

.app_list_wrap:after {
    display: block;
    content: "";
    clear: both;
}

.app_list_wrap>article {
    float: left;
    width: 426px;
    margin: 0 20px 20px 0;
    background: #fff;
    border-radius: 10px;
    position: relative;
    box-shadow: 2px 2px 4px rgba(0, 0, 0, .1);
}

.app_list_wrap>article:nth-child(3n) {
    margin-right: 0;
}

.app_list_wrap>article a {
    display: block;
    padding: 30px;
    padding-left: 86px;
}

.app_list_wrap .app-box .app-icon {
    display: block;
    width: 52px;
    height: 52px;
    font-size: 48px;
    line-height: 52px;
    color: #fff;
    text-align: center;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    position: absolute;
    top: 25px;
    left: 0;
}

.app_list_wrap .app-box .app-icon:before {
    filter: drop-shadow(-1px -1px 0 rgba(0, 0, 0, 0.2));
}

.app_list_wrap .app-box .tit {
    font-size: 20px;
    color: #000;
    font-weight: 600;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.app-rate-wrap {
    margin-top: 10px;
    font-size: 0;
}

.app-rate-wrap>i {
    display: inline-block;
    width: 11px;
    height: 11px;
    background: url(/app/design/images/store/icon_star_off.png);
    margin-right: 3px;
}

.app-rate-wrap>i.on {
    background: url(/app/design/images/store/icon_star_on.png);
}

.app_list_wrap .app-box .app-info {
    margin-top: 2px;
    font-size: 0;
}

.app_list_wrap .app-box .app-info li {
    display: inline-block;
    padding: 0 10px;
    border-right: 1px solid #d3d7dd;
    font-size: 12px;
    color: #9da2ac;
    margin-bottom: 3px;
}

.app_list_wrap .app-box .app-info li:first-child {
    padding-left: 0;
}

.app_list_wrap .app-box .app-info li:last-child {
    border-right: 0;
    padding-right: 0;
    font-size: 13px;
    color: #888;
}

.app_list_wrap .app-box .app-info li:last-child .num {
    font-size: 20px;
    color: #333;
    font-weight: 500;
    margin-left: 10px;
    vertical-align: bottom;
}

.app_list_wrap .app-box .desc {
    margin: 22px 0 22px 0;
    color: #000;
    font-weight: 300;
    line-height: 19px;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height: 19px;
    height: 38px;
    overflow: hidden;
}

.app_list_wrap .app-box .tags {
    font-size: 13px;
    font-weight: 300;
    color: #45a7d0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.app_list_wrap .app-box .category {
    margin-top: 12px;
    font-size: 0;
}

.btn-user-stu,
.btn-user-pro,
.btn-user-mng {
    display: inline-block;
    padding: 6px 8px;
    background: #fff;
    border-radius: 4px;
    font-size: 13px;
    font-weight: 300;
    color: #666;
    margin-right: 2px;
}

.btn-user-stu {
    background: #4873ca;
    color: #fff;
}

.btn-user-pro {
    background: #6954b8;
    color: #fff;
}

.btn-user-mng {
    background: #8744ae;
    color: #fff;
}

.app_list_wrap .app-box .category li:last-child {
    float: right;
    margin-right: 0;
    border: 1px solid #d7d7d7;
}

.app_detail:after {
    display: block;
    content: "";
    clear: both;
}

.app_detail .left-area {
    float: left;
    width: 368px;
    padding: 0 40px 0 8px;
}

.app_detail .left-area .app-title {
    padding-left: 118px;
    position: relative;
}

.app_detail .left-area .app-title .app-icon {
    display: block;
    width: 88px;
    height: 88px;
    border-radius: 4px;
    text-align: center;
    position: absolute;
    top: 0;
    left: 0;
}

.app_detail .left-area .app-title .app-icon:before {
    font-size: 80px;
    color: #fff;
    line-height: 88px;
}

.app_detail .left-area .app-title .tit {
    font-size: 24px;
    line-height: 34px;
    font-weight: 700;
    color: #000;
    padding-top: 5px;
}

.app_detail .left-area .app-title .app-info {
    margin-top: 14px;
    font-size: 0;
}

.app_detail .left-area .app-title .app-info li {
    display: inline-block;
    padding: 0 10px;
    border-right: 1px solid #d3d7dd;
    font-size: 12px;
    color: #9da2ac;
    margin-bottom: 3px;
}

.app_detail .left-area .app-title .app-info li:first-child {
    padding-left: 0;
}

.app_detail .left-area .app-title .app-info li:last-child {
    border-right: 0;
    padding-right: 0;
}

.app_detail .left-area .app-title .users {
    font-weight: 300;
    color: #888;
    margin-top: 12px;
}

.app_detail .left-area .app-title .users span {
    font-weight: 600;
    color: #eb3d3d;
}

.app_detail .left-area .app-control {
    margin-top: 40px;
    font-size: 0;
}

.app_detail .left-area .app-control a {
    display: inline-block;
    width: 155px;
    height: 45px;
    line-height: 45px;
    border-radius: 45px;
    font-size: 15px;
    font-weight: 300;
    text-align: center;
}

.app_detail .left-area .app-control .btn-app-stop {
    background: #f2f4f6;
    border: 1px solid #e6e9ec;
    margin-right: 10px;
}

.app_detail .left-area .app-control .btn-app-stop:before {
    font-size: 22px;
    color: #f64040;
    margin-right: 4px;
    vertical-align: top;
}

.app_detail .left-area .app-control .btn-app-stop .toggle-switch {
    width: 32px;
    height: 16px;
    vertical-align: middle;
    margin-bottom: 3px !important;
    margin-left: 10px;
}

.app_detail .left-area .app-control .btn-app-stop .toggle-switch .slider:before {
    width: 12px;
    height: 12px;
}

.app_detail .left-area .app-control .btn-app-stop .toggle-switch input:checked+.slider:before {
    transform: translateX(16px);
    -webkit-transform: translateX(16px);
    -moz-transform: translateX(16px);
}

.app_detail .left-area .app-control .btn-widget-set {
    background: #e72956;
    color: #fff;
}

.app_detail .left-area .app-control .btn-widget-set:before {
    font-size: 22px;
    margin-right: 10px;
    vertical-align: top;
}

.app_detail .left-area .app-info {
    margin-top: 170px;
}

.app_detail .left-area .app-info h5 {
    font-size: 16px;
    font-weight: 500;
    color: #333;
    margin-bottom: 24px;
}

.app_detail .left-area .app-info .list-type06 dd {
    max-width: 260px;
}

.app_detail .left-area .app-info .list-type06 strong {
    font-weight: 600;
}

.app_detail .left-area .app-info .list-type06 .btn-app-state {
    display: block;
    height: 23px;
    font-size: 12px;
    line-height: 23px;
    border-radius: 23px;
    background: #30a2dd;
    color: #fff;
    padding: 0 10px;
}

.app_detail .left-area .app-info .list-type06 span[class^="btn-user-"] {
    line-height: 13px;
}

.app_detail .left-area .app-info .list-type06 span[class^="icon-"] {
    font-size: 24px;
    color: #7b7b7b;
    margin-right: 20px;
}

.app_detail .right-area {
    float: left;
    width: 856px;
    border-left: 1px solid #dbdde2;
    padding: 0 8px 0 40px;
}

.app_detail .right-area>article {
    padding: 40px 0;
    border-bottom: 1px solid #dbdde2;
    position: relative;
}

.app_detail .right-area>article:last-child {
    border-bottom: 0;
}

.app_detail .right-area>article h4 {
    font-size: 18px;
    color: #333;
    font-weight: 500;
}

.app_detail .right-area>article h4 span {
    display: inline-block;
    width: 27px;
    height: 27px;
    background: #364085;
    border-radius: 50%;
    text-align: center;
    font-size: 14px;
    color: #fff;
    line-height: 27px;
    margin-right: 8px;
}

.app_detail .right-area>article>.cont-inner {
    padding-left: 38px;
    margin-top: 12px;
}

.app_detail .right-area>article .cont-inner>p {
    font-family: 'Noto Sans KR', sans-serif;
    font-weight: 300;
    line-height: 22px;
}

.app_detail .right-area>article .cont-inner>p.description {
    font-size: 15px;
}

.app_detail .right-area>article .btn-view-all {
    display: inline-block;
    height: 16px;
    color: #999;
    font-weight: 300;
    padding-left: 23px;
    background: url(/app/design/images/store/icon_menu01.gif) center left no-repeat;
    position: absolute;
    top: 40px;
    right: 10px;
}

.point-wrap {
    margin-top: 30px;
}

.point-wrap>.point-box {
    float: left;
    width: 140px;
    margin-right: 70px;
}

.point-wrap>.point-box>.point-txt {
    display: block;
    font-size: 90px;
    color: #333;
    line-height: 1.0em;
    text-align: center;
    font-family: 'Montserrat';
    font-weight: 300;
    font-style: italic;
    padding-bottom: 23px
}

.point-wrap>.point-box>.total-txt {
    font-size: 15px;
    color: #333;
    line-height: 1.4em;
    font-family: 'Noto Sans KR';
}

.point-wrap>.graph-box {
    overflow: hidden;
}

.point-wrap>.graph-box>li {
    overflow: hidden;
}

.point-wrap>.graph-box>li>.point {
    float: left;
    font-size: 15px;
    width: 25px;
    color: #333;
    line-height: 1.4em;
    font-weight: 500;
    font-family: 'Montserrat';
    padding-right: 15px;
    background: url(/app/design/images/store/icon_star.png) no-repeat right center;
}

.point-wrap>.graph-box>li>.graph-line {
    float: left;
    width: 90%;
    padding: 6px 8px;
}

.point-wrap>.graph-box>li>.graph-line>.graph {
    display: inline-block;
    height: 11px;
    border-radius: 6px;
    text-align: right;
}

.point-wrap>.graph-box>li>.graph-line>.graph.five {
    background-color: #364085;
}

.point-wrap>.graph-box>li>.graph-line>.graph.four {
    background-color: #5963a7;
}

.point-wrap>.graph-box>li>.graph-line>.graph.three {
    background-color: #7983c4;
}

.point-wrap>.graph-box>li>.graph-line>.graph.two {
    background-color: #a5acda;
}

.point-wrap>.graph-box>li>.graph-line>.graph.one {
    background-color: #c5caec;
}

.point-wrap>.graph-box>li>.graph-line>.graph>.num {
    font-size: 15px;
    color: #333;
    line-height: 1.4em;
    font-weight: 500;
    font-family: 'Montserrat';
    margin-right: -25px;
    margin-top: -5px;
    display: block;
}

.review-write {
    margin-top: 60px;
}

.review-write h5 {
    font-size: 15px;
    columns: #333333;
    font-weight: 300;
    margin-bottom: 13px;
}

.review-write h5 span {
    font-size: 20px;
    color: #000000;
    margin-left: 6px;
}

.review-write textarea {
    background: #f8f8f8;
    border-color: #e0e0e0;
}

.review-list {
    margin-top: 60px;
}

.review-list h5 {
    font-size: 18px;
    color: #333;
    font-weight: 500;
    margin-bottom: 15px;
    position: relative;
}

.app-review-sort {
    position: absolute;
    right: 0;
    top: 5px;
}

.app-review-sort>.sort {
    float: left;
    font-size: 13px;
    color: #666;
    font-weight: 300;
    line-height: 1.4em;
    margin-left: 20px;
}

.app-review-sort>.sort span {
    margin-left: 4px;
    color: #b0b0b0;
}

.app-review-list {
    overflow: hidden;
}

.app-review-list>li {
    float: left;
    width: 400px;
    height: 200px;
    border: 1px solid #e0e0e0;
    border-radius: 8px;
    margin: 5px 0;
}

.app-review-list>li:nth-child(2n) {
    margin-left: 7px;
}

.app-review-list>li>a {
    display: block;
    padding: 35px;
    padding-left: 95px;
    position: relative;
}

.app-review-list .photo {
    display: block;
    width: 50px;
    height: 50px;
    border-radius: 50%;
    overflow: hidden;
    position: absolute;
    top: 30px;
    left: 30px;
}

.app-review-list .tit {
    font-size: 16px;
    line-height: 20px;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.app-review-list .app-rate-wrap {
    margin-top: 5px;
}

.app-review-list .user-line {
    color: #888;
    line-height: 1.4em;
    padding-top: 3px;
    margin: 16px 0;
}

.app-review-list .user-line>.name {
    float: left;
    font-weight: 300;
    color: #777;
    padding-right: 10px;
}

.app-review-list .user-line>.date {
    position: relative;
    padding-left: 10px;
}

.app-review-list .user-line>.date:before {
    position: absolute;
    content: '';
    width: 1px;
    height: 9px;
    left: 0;
    top: 5px;
    background-color: #bebebe;
}

.app-review-list .review-txt {
    font-size: 15px;
    line-height: 20px;
    font-weight: 300;
    color: #111;
}

.app-review-more {
    margin-top: 20px;
    text-align: right;
}

.app-review-more>a {
    font-size: 14px;
    color: #364085;
    line-height: 1.4em;
    font-weight: 600;
}

.app-review-more>a .icon-chevron-right {
    font-size: 18px;
    margin-left: 6px;
}

.app_star {
    padding: 5px 0;
}

.app_star>span.on {
    display: inline-block;
    width: 12px;
    height: 12px;
    margin: 1px;
    background: url(..//app/design/images/store/sub/app/icon_star_on.png) no-repeat;
}

.app_star>span.off {
    display: inline-block;
    width: 12px;
    height: 12px;
    margin: 1px;
    background: url(..//app/design/images/store/sub/app/icon_star_off.png) no-repeat;
}

.app_star>span.half {
    display: inline-block;
    width: 12px;
    height: 12px;
    margin: 1px;
    background: url(..//app/design/images/store/sub/app/icon_star_half.png) no-repeat;
}

@media only screen and (max-width:1280px) {
    .app_list_wrap>article {
        width: 47%;
        margin-left: 1.5%;
        margin-right: 1.5%;
    }
    .app_list_wrap>article:nth-child(3n) {
        margin-right: 1.5%;
    }
    .app_list_wrap>article a {
        padding-left: 74px;
    }
    .store_top .search-wrap {
        margin-top: 50px;
    }
    .store_top .search-wrap label,
    .store_top .search-wrap select,
    .store_top .search-wrap input,
    .store_top .search-wrap a {
        margin-bottom: 5px;
    }
    .app-rate-wrap>i {
        background: url(/app/design/images/store/icon_star_off_2x.png);
        background-size: 11px 11px;
    }
    .app-rate-wrap>i.on {
        background: url(/app/design/images/store/icon_star_on_2x.png);
        background-size: 11px 11px;
    }
    .app_detail .right-area>article .btn-view-all {
        background: url(/app/design/images/store/icon_menu01_2x.gif) no-repeat;
        background-size: 16px 16px;
    }
    .app_detail .left-area,
    .app_detail .right-area {
        float: none;
        width: 100%;
        padding: 0;
        border: 0;
    }
    .app_detail .right-area {
        border-top: 1px solid #dbdde2;
        margin-top: 40px;
    }
    .app_detail .left-area .app-info {
        margin-top: 40px;
    }
    .app-review-list>li {
        width: 49%;
    }
    .app-review-list>li:nth-child(2n) {
        margin-left: 2%;
    }
    .app-review-list>li>a {
        padding: 25px;
        padding-left: 85px;
    }
    .app-review-list .photo {
        top: 25px;
        left: 20px;
    }
}

@media only screen and (max-width:1024px) {
    .app_list_wrap>article {
        width: 100%;
        margin-left: 0;
        margin-right: 0;
    }
    .app_list_wrap>article:nth-child(3n) {
        margin-right: 0;
    }
    .app_detail .left-area .app-info {
        margin-top: 40px;
    }
    .app-review-list>li {
        width: 100%;
        height: auto;
    }
    .app-review-list>li:nth-child(2n) {
        margin-left: 0;
    }
}

@media only screen and (max-width:640px) {
    .sub_cont_wrap .store_top {
        height: auto;
        background-size: cover;
        padding: 0 20px;
    }
    .store_top .search-wrap {
        padding: 0 20px 30px 30px;
    }
    .jui .store_top h3 {
        font-size: 42px;
        line-height: 1.2;
        margin-top: 60px;
    }
    .app_detail .left-area {
        margin-top: 10px;
    }
    .app_detail .left-area .app-control a {
        width: 46%;
    }
    .app_detail .left-area .app-control .btn-app-stop {
        margin-right: 4%;
    }
    .point-wrap>.point-box {
        float: none;
        width: 100%;
        margin-bottom: 25px;
        text-align: center;
    }
}