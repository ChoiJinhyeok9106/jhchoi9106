.stu-counsel-list {
    position: relative;
    display: inline-block;
    width: 100%;
    border: 1px solid #dcdff1;
    background-color: #fff;
    border-radius: 8px;
    margin: 4px 0;
    transition: .3s;
}
.stu-counsel-list:hover {
    border-color:#9ea4d0;
    -webkit-box-shadow: 0px 0px 3px 0px rgba(158, 164, 208, 1);
    -moz-box-shadow: 0px 0px 3px 0px rgba(158, 164, 208, 1);
    box-shadow: 0px 0px 3px 0px rgba(158, 164, 208, 1);
}
.stu-counsel-list a {
    display: inline-block;
    padding: 20px 20px;
    width:100%;
    font-family:'noto sans kr';
}
.stu-counsel-list .title {
    display: block;
    font-size: 15px;
    color: #333;
    margin-bottom: 8px;
}
.stu-counsel-list .name {
    display: inline-block;
    font-size: 14px;
    color: #888;
}
.stu-counsel-list .date {
    float:right;
    display: inline-block;
    font-size: 14px;
    color: #9ea4d0;
}

/*설문*/
.counsel-survey-box {
    border-radius:8px;
    background-color:#fff;
    border:1px solid #666faf;
    margin:10px 0;
}
.counsel-survey-box .header-line {
    display: table;
    width:100%;
    background-color:#f1f2f8;
    border-top-right-radius:8px;
    border-top-left-radius:8px;
    border-bottom:1px solid #cfd2e0;
}
.counsel-survey-box .header-line .number {
    float:left;
    margin-right:10px;
    font-size:26px;
    color:#fff;
    font-weight:500;
    background-color:#666faf;
    width:50px;
    height:50px;
    text-align:center;
    line-height:50px;
    border-top-left-radius:6px;
}
.counsel-survey-box .header-line .text {
    display: table-cell;
    width:100%;
    font-size:16px;
    color:#364085;
    vertical-align: middle;
    line-height: 1.4em;
}
.counsel-survey-box .mid-area {
    background-color:#fff;
    padding:20px;
    border-bottom-right-radius:8px;
    border-bottom-left-radius:8px;
}
.counsel-survey-box .mid-area .sub-title-line {
    position:relative;
    display: table;
}
.counsel-survey-box .mid-area .sub-title-line .number {
    float:left;
    margin-right:10px;
    font-size:12px;
    color:#fff;
    font-weight:600;
    background-color:#666faf;
    width:24px;
    height:24px;
    text-align:center;
    line-height:24px;
    border-radius:50%;
}
.counsel-survey-box .mid-area .sub-title-line .text {
    display:table-cell;
    font-size:15px;
    color:#444;
    vertical-align: middle;
    line-height: 1.4em;
}
.counsel-survey-box .mid-area .survey-cont-box {
    display: inline-block;
    margin:10px 0 10px 30px;
    padding:0 10px;
    background-color:#f1f2f8;
    border-radius:8px;
}

