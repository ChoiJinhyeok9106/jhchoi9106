<script>
$(function($){
	//탭메뉴
	$(".tab-content").hide();
	$(".tab-content:first").show();
	$(".counsel-tab div a").click(function() {
		$(".counsel-tab div a").removeClass("active");
		$(this).addClass("active");
		$(".tab-content").hide()
		var activeTab = $(this).parent().attr("rel");
		$("#"+activeTab).fadeIn()
	});

       //상담하기 레이어
       $('.btn-counsel').on('click', function(){
           $('.counseling-aside').addClass('on');
           $('.counseling-aside').css({'transition':'.3s'});
       });
       $('.counseling-aside .btn-close').on('click', function(){
           $('.counseling-aside').removeClass('on');
           $('.counseling-aside').css({'transition':'.3s'});
       })

       //상담이력 열고 닫기
       $('.counsel-history-list .question').on('click', function() {
           if ($(this).hasClass('on')) {
               $(this).removeClass('on');
               $(this).siblings('.answer').slideUp();

           } else {
               $('.counsel-history-list .answer').slideUp();
               $('.counsel-history-list .question').removeClass('on');
               $(this).siblings('.answer').slideDown();
               $(this).addClass('on');
           }
       });
       $('.counsel-history-total').on('click', function() {
           if ($(this).text() == '펼쳐보기') {
               switchtext($(this), '모두닫기');
               $('.counsel-history-list .answer').slideDown();
           } else {
               switchtext($(this), '펼쳐보기');
               $('.counsel-history-list .answer').slideUp();
           }
       });
});

   function switchtext(target, value) {
       target.contents().filter(function() {
           return this.nodeType == 3;
       }).remove().end().end().append(value);
   }


</script>