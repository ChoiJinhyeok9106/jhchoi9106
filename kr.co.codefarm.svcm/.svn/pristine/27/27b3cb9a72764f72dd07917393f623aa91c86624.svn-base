@charset "utf-8";

@import url('https://fonts.googleapis.com/css?family=Montserrat:100,200,300,400,500,600,700,800,900');
@import url('https://fonts.googleapis.com/css?family=Noto+Sans+KR:100,300,400,500,700,900&subset=korean');

/*common*/

html,
body {
    min-height: 100%;
}

body {
    text-align: left;
    font-size: 14px;
    color: #444;
    line-height: 1;
    font-family: 'Montserrat', 'Noto Sans KR', sans-serif;
    font-weight: 400;
    letter-spacing: 0px;
}

body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
span,
form,
fieldset,
p,
button,
address,
table,
tbody,
tfoot,
thead,
th,
tr,
tdvideo,
tt {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

header,
article,
aside,
canvas,
details,
figcaption,
figure,
footer,
hgroup,
menu,
main,
nav,
section,
summary,
mark,
time {
    display: block;
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

input,
button,
textarea,
select {
    background-color: #fff;
    border-radius: 0;
    box-sizing: border-box;
}


/*-webkit-appearance: none*/

button,
textarea {
    -webkit-appearance: none
}

img,
fieldset,
iframe {
    border: 0 none
}

li {
    list-style: none
}

img,
input,
select,
button,
textarea {
    vertical-align: middle;
    box-sizing: border-box;
}

img {
    max-width: 100%
}

em,
address,
optgroup {
    font-style: normal
}

label,
button {
    cursor: pointer
}

label {
    /* top: 2px; */
    position: relative;
}

button {
    margin: 0;
    padding: 0;
    border: 0;
    background: none
}

a {
    box-sizing: border-box
}

a:link {
    text-decoration: none;
    color: #3a3a3a;
}

a:visited {
    color: #333
}

a:hover,
a:focus,
a:active {
    text-decoration: none;
    color: #3a3a3a
}

hr {
    display: none
}

legend,
caption {
    font-size: 0;
    width: 0;
    height: 0;
    line-height: 0;
    overflow: hidden;
    text-indent: -9999px
}

p {
    text-align: left;
    font-weight: 400;
    word-break: keep-all
}

table {
    width: 100%;
    font-weight: 400;
    border-spacing: 0
}


/*skip 네비게이션*/

#skip_navi {
    position: absolute;
    top: 0;
    height: 0px;
    width: 100%;
    z-index: 1000;
}

#skip_navi a {
    display: block;
    height: 1px;
    width: 1px;
    margin-bottom: -1px;
    overflow: hidden;
    text-align: center;
    color: #fff;
    white-space: nowrap;
}

#skip_navi a:focus,
#skip_navi a:active {
    display: block;
    top: 0;
    width: 100%;
    height: 45px;
    padding: 10px 0;
    background: #021b87;
}

#wrap {
    position: relative;
    width: 100%;
    height: 100%;
}



#content-wrap {
    position: relative;
    width:320px;
    border-radius:8px;
}

#header {
    width:100%;
    height:60px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    background-color:#364085;
    text-align: center;
}
#header h1 {
    position: relative;
    display: inline-block;
    font-size:26px;
    font-weight:700;
    color:#fff;
    font-family:'montserrat';
    padding-left:30px;
    margin-top:17px;
}
#header h1:before {
    position: absolute;
    content: '\e9d5';
    top:2px;
    left:0;
    font-size:24px;
    color:#fff;
    font-family: 'feather';
    font-weight: 400;
}


.contents {
    position: relative;
    background-color:#fff;
    padding:20px 15px;
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 8px;
}
.contents .take-wrap {
    position: relative;
}
.contents .take-wrap h2 {
    position: relative;
    float:left;
    font-size:16px;
    color:#000;
    font-family:'noto sans kr';
    padding-left:30px;
    margin: 0px !important;
}
.contents .take-wrap h2:before {
    position: absolute;
    content: '\e9fa';
    left:5px;
    top:-1px;
    font-family: 'feather';
    color:#424b8c;
    font-size:20px;

}
.contents .take-wrap .total {
    float:right;
    font-size:14px;
    color:#000;
    font-weight:300;
    font-family:'noto sans kr';
    padding-right:5px;
}
.contents .take-wrap .take-people-box {
    display: inline-block;
    width:100%;
    padding:8px;
    background-color:#eceef7;
    border-radius:8px;
    margin-top:10px;
}
.contents .take-wrap .take-people-box .take-people-inner {
    overflow-y: auto;
    overflow-x: hidden;
    height:100px;
    font-size: 0;
}
.contents .take-wrap .take-people-box .take-people-inner .sel-people {
    position: relative;
    display: block;
    font-size:13px;
    color:#364085;
    padding:8px 5px 8px 20px;
    border:1px solid #d7daf1;
    border-radius:5em;
    background-color:#fff;
    margin:2px 2px;
    font-family:'noto sans kr';
}
.contents .take-wrap .take-people-box .take-people-inner .sel-people .tel {
    display: inline-block;
    color:#444;
    margin-left:15px;
}
.contents .take-wrap .take-people-box .take-people-inner .sel-people a {
    position: absolute;
    top:4px;
    right:5px;
    color:#9ea4d0;
    font-size:20px;
    margin-left:10px;
}
.contents .message-wrap {
    position: relative;
    margin-top:20px;
    text-align: right;
}
.contents .message-wrap .byte {
    font-size:12px;
    color:#aaa;
    font-weight: 300;
    font-family:'noto sans kr';
    padding-right:5px;
}
.contents .message-wrap .byte span {
    padding:0 3px;
}
.contents .message-wrap .message-box {
    display: inline-block;
    width:100%;
    padding:15px;
    border:1px solid #9ea4d0;
    border-radius:8px;
    margin-top:5px;
}
.contents .message-wrap .message-box .message-inner {
    overflow-y: auto;
    overflow-x: hidden;
    height:180px;
    text-align: left;
    border:0;
    padding:0;
}
.contents .message-wrap .message-box .message-inner:focus {
    border:0;
    box-shadow: none;
}

[contenteditable=true]:empty:before{
  content: attr(placeholder);
  display: block; /* For Firefox */
}

div[contenteditable=true] {
  color : #888;
  font-size: 13px;
  width: 100%;
  font-weight:300;
  font-family:'noto sans kr';
}

.contents .send-wrap {
    position: relative;
    display: inline-block;
    width:100%;
    margin-top:20px;
    border-bottom:2px solid #666faf;
}
.contents .send-wrap h2 {
    position: relative;
    float:left;
    font-size:16px;
    color:#000;
    font-family:'noto sans kr';
    padding-left:30px;
    padding-top:10px;
    margin: 0px !important;
}
.contents .send-wrap h2:before {
    position: absolute;
    content: '\e9f6';
    left:5px;
    top:8px;
    font-family: 'feather';
    color:#424b8c;
    font-size:20px;

}
.contents .send-wrap input.send-tel {
    float:right;
    width:50%;
    font-size:16px;
    color:#000;
    font-family:'noto sans kr';
    text-align: right;
    border:0;
}
.contents .send-wrap input.send-tel:focus {
    border:0;
    box-shadow: none;
}

.contents .btn-wrap {
    position: relative;
    display: inline-block;
    width:100%;
    padding:30px 0 0;
    text-align: center;
}
.contents .btn-wrap .btn-send {
    display: inline-block;
    padding:10px 30px 12px;
    font-size:16px;
    color:#fff;
    font-family:'noto sans kr';
    background-color:#364085;
    border-radius:5em;
}

#footer {
    position: relative;
    width:100%;
    height:54px;
}
