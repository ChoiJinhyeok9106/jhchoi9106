package kr.co.codefarm.svcm.commons.properties;

import org.springframework.boot.context.properties.ConfigurationProperties;

import java.util.List;

/**
 * @author Dki_s
 *
 */
@ConfigurationProperties("sysprop")
public class SystemProp {
	
	private String sessionKeyId;
	private String sessionKeyCurrentPg;
	private String sessionDupCheckType;
	
	private String uploadDir;
	private String uploadPath;
	private String uploadFixedName;
	private String uploadMaxSize;
	private String uploadErr;
	private String uploadCnt;
	private List<String> uploadAllowMimetypes;

	private String filemapPath;
	private String filemapName;
	private String filemapType;
	private String filemapSize;
	
	private String suffixListParam;
	private String defaultPageSize;
	private String downloadFile;
	private String downloadFileNm;
	
	private String suffixJsonListParam;
	
	public String getSessionKeyId() {
		return sessionKeyId;
	}
	public void setSessionKeyId(String sessionKeyId) {
		this.sessionKeyId = sessionKeyId;
	}
	public String getSessionKeyCurrentPg() {
		return sessionKeyCurrentPg;
	}
	public void setSessionKeyCurrentPg(String sessionKeyCurrentPg) {
		this.sessionKeyCurrentPg = sessionKeyCurrentPg;
	}
	public String getSessionDupCheckType() {
		return sessionDupCheckType;
	}
	public void setSessionDupCheckType(String sessionDupCheckType) {
		this.sessionDupCheckType = sessionDupCheckType;
	}
	public String getUploadDir() {
		return uploadDir;
	}
	public void setUploadDir(String uploadDir) {
		this.uploadDir = uploadDir;
	}
	public String getUploadPath() {
		return uploadPath;
	}
	public void setUploadPath(String uploadPath) {
		this.uploadPath = uploadPath;
	}
	public String getUploadFixedName() {
		return uploadFixedName;
	}
	public void setUploadFixedName(String uploadFixedName) {
		this.uploadFixedName = uploadFixedName;
	}
	public String getUploadMaxSize() {
		return uploadMaxSize;
	}
	public void setUploadMaxSize(String uploadMaxSize) {
		this.uploadMaxSize = uploadMaxSize;
	}
	public String getUploadErr() {
		return uploadErr;
	}
	public void setUploadErr(String uploadErr) {
		this.uploadErr = uploadErr;
	}
	public String getUploadCnt() {
		return uploadCnt;
	}
	public void setUploadCnt(String uploadCnt) {
		this.uploadCnt = uploadCnt;
	}
	public List<String> getUploadAllowMimetypes() {
		return uploadAllowMimetypes;
	}
	public void setUploadAllowMimetypes(List<String> uploadAllowMimetypes) {
		this.uploadAllowMimetypes = uploadAllowMimetypes;
	}
	public String getFilemapPath() {
		return filemapPath;
	}
	public void setFilemapPath(String filemapPath) {
		this.filemapPath = filemapPath;
	}
	public String getFilemapName() {
		return filemapName;
	}
	public void setFilemapName(String filemapName) {
		this.filemapName = filemapName;
	}
	public String getFilemapType() {
		return filemapType;
	}
	public void setFilemapType(String filemapType) {
		this.filemapType = filemapType;
	}
	public String getFilemapSize() {
		return filemapSize;
	}
	public void setFilemapSize(String filemapSize) {
		this.filemapSize = filemapSize;
	}
	public String getSuffixListParam() {
		return suffixListParam;
	}
	public void setSuffixListParam(String suffixListParam) {
		this.suffixListParam = suffixListParam;
	}
	public String getDefaultPageSize() {
		return defaultPageSize;
	}
	public void setDefaultPageSize(String defaultPageSize) {
		this.defaultPageSize = defaultPageSize;
	}
	public String getDownloadFile() {
		return downloadFile;
	}
	public void setDownloadFile(String downloadFile) {
		this.downloadFile = downloadFile;
	}
	public String getDownloadFileNm() {
		return downloadFileNm;
	}
	public void setDownloadFileNm(String downloadFileNm) {
		this.downloadFileNm = downloadFileNm;
	}
	public String getSuffixJsonListParam() {
		return suffixJsonListParam;
	}
	public void setSuffixJsonListParam(String suffixJsonListParam) {
		this.suffixJsonListParam = suffixJsonListParam;
	}
	
}
